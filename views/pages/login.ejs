        <div id="login_control">
            <input type="button" id="btn_login" value="Login" onclick="login();"/>
            <a id="signup" href="<%= link%>">Join us!</a>
            <a id="forget_pwd" href="<%= link%>">Forget Passwordï¼Ÿ</a>
